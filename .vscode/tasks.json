{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "run-local.sh",
            "type": "shell",
            "command": "./run-local.sh",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "isBackground": true,
            "problemMatcher": {
                "owner": "custom",
                "pattern": {
                    "regexp": "^(.*)$"
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "^Launching Flutter emulator.*",
                    "endsPattern": "launch\\.json flutter debug configuration is ready to use\\."
                }
            }
        }
    ]
}
